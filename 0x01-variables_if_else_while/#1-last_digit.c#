#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/**
 * main - Entry point
 *
 * Return : Always 0 (Success)
 */
int main(void)
{
	int lst;

	lst = m % 10;

	if (lst > 5)
	{
	printf("Last digit of %i is %i and is greater than 5\n", m, lst);
	}
	else if (lst == 0)
	{
	printf("Last digit of %d is %d and is 0\n", m, lst);
	}
	else
	{
	printf("Last digit of %d is %d and is less than 6 and not 0\n", m, lst);
	}
}
/**
